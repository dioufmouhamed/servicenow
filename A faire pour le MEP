//mettre a jour les ci                 
grAlmAsset.query();
while (grAlmAsset.next()) {
	updateCi(grAlmAsset.ci,grAlmAsset.sys_id , grAlmAsset.ci.sys_class_name);
}

function updateCi(ci,asset, classe){
var grCCNAS = new GlideRecord(classe);
if (grCCNAS.get(ci)) {
	grCCNAS.asset = asset;
	grCCNAS.update();
}
}